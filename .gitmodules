[submodule "ssh"]
	path = ssh/.ssh/ssh_homelab_hosts
	url = git@github.com:roadmanict/ssh-config.git
[submodule "alacritty/rose-pine"]
	path = alacritty/.config/alacritty/rose-pine
	url = https://git::@github.com/rose-pine/alacritty.git
[submodule "tmux/plugins/tmux"]
	path = tmux/.config/tmux/plugins/tmux
