[submodule "tmux/plugins/tpm"]
	path = tmux/.config/tmux/plugins/tpm
	url = https://git::@github.com/tmux-plugins/tpm
[submodule "zsh/.zprezto"]
	path = zsh/.config/zsh/.zprezto
